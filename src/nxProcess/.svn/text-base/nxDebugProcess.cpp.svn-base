#include "nxDebugProcess.hpp"

nxDebugProcess::nxDebugProcess( ) :
	nxProcess( NX_PROC_DEBUG )
{
}

nxDebugProcess::~nxDebugProcess( )
{
}


void nxDebugProcess::VOnUpdate( const int iDelta )
{
	nxProcess::VOnUpdate( iDelta );
	if ( m_bIsActive )
	{
		log(NX_LOG_ERROR, "nxDebugProcess here");
		VKill();
	}
}
